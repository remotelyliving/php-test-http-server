{
    "name": "remotelyliving/php-test-server",
    "description": "A lightweight test server for real world http interaction built on the PHP native HTTP server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Thomas",
            "email": "christian.h.thomas@me.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symfony/process": "^5.2",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^3.1",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.4",
        "vimeo/psalm": "^4.6",
        "phpunit/phpunit": "^9.5",
        "nyholm/psr7": "^1.4",
        "kriswallsmith/buzz": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "RemotelyLiving\\PHPTestHttpServer\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RemotelyLiving\\PHPTestHttpServer\\Tests\\" : "tests/"
        }
    }
}
